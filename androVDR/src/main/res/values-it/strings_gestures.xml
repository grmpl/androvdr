<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->
    
    <!-- Title, name of the activity used to create a gesture
         and displayed in menu to start the activity to create a gesture -->
    <string name="label_create_gesture">Crea un gesto</string>

    <!-- Buttons -->
    <skip />
    <!-- Label, button used to add a gesture -->
    <string name="button_add">Aggiungi gesto</string>
    <!-- Label, button used to reload all gestures -->
    <string name="button_reload">Ricarica</string>
    <!-- Label, button used to cancel the operation of adding a gesture -->
    <string name="button_discard">Annulla</string>
    <!-- Label, button used to save a gesture newly created -->
    <string name="button_done">Completato</string>
    
    <!-- Gestures -->
    <skip />
    <!-- Label, tell the user to enter a gesture -->
    <string name="prompt">Scatena la tua creativit√†!</string>
    <!-- Label, displayed in menu  to start the activity for enter gestures -->
    <string name="gestures">Gesti</string>
    <!-- Label, prompt asking the user to enter the name of the gesture -->
    <string name="prompt_gesture_name">Nome</string>
    <!-- Error message, informs the user he needs to enter a name before saving a gesture -->
    <string name="error_missing_name">Devi inserire un nome</string>
    <!-- success message, tells the user where the gesture was saved -->
    <string name="save_success">Gesto salvato in %s</string>
    <!-- Buttons in Rename gesture dialog box -->
    <string name="rename_action">OK</string>
    <!-- Buttons in Rename gesture dialog box -->
    <string name="cancel_action">Annulla</string>
    <!-- Message displayed when the user opens the gestures settings screen -->
    <string name="gestures_loading">Caricamento gesti...</string>
    <!-- Message displayed when the user has no gestures -->
    <string name="gestures_empty">Nessun gesto</string>
    <!-- Title of the screen used to view/manage gestures -->
    <string name="gestures_activity">Gesti</string>
    <!-- Noun, menu item used to rename a gesture -->
    <string name="gestures_rename">Rinomina</string>
    <!-- Noun, menu item used to remove a gesture -->
    <string name="gestures_delete">Elimina</string>
    <!-- Message displayed when a gesture is successfully deleted -->
    <string name="gestures_delete_success">Gesto eliminato</string>
    <!-- Title of dialog box -->
    <string name="gestures_rename_title">Rinomina gesto</string>
    <!-- Label of gesture name field in Rename gesture dialog box -->
    <string name="gestures_rename_label">Nome gesto</string>
    <!-- Message, displayed when the sdcard cannot be found, 1st parameter is the name of the file that stores the gestures -->
    <string name="gestures_error_loading">Impossibile caricare %s. Assicurati di aver montato la scheda SD.</string>
    
    <string name="no_gestures">"Gesti mancanti. Devi prima crearne qualcuno."</string>
</resources>
